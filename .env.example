# Example environment configuration for Panda Cloud
# Copy this to .env and update with your credentials

# User 1 Configuration
USER_demo_PASSWORD="demo123"
USER_demo_AWS_ACCESS_KEY_ID="your_access_key_here"
USER_demo_AWS_SECRET_ACCESS_KEY="your_secret_key_here"
USER_demo_AWS_S3_BUCKET="your_bucket_name"
USER_demo_AWS_S3_ENDPOINT="s3.tebi.io"

# Global S3 Configuration (optional - can be overridden per user)
AWS_S3_REGION="us-east-1"
AWS_S3_ENDPOINT="s3.tebi.io"

# Add more users by following the same pattern:
# USER_{username}_PASSWORD="password"
# USER_{username}_AWS_ACCESS_KEY_ID="access_key"
# USER_{username}_AWS_SECRET_ACCESS_KEY="secret_key"  
# USER_{username}_AWS_S3_BUCKET="bucket_name"
# USER_{username}_AWS_S3_ENDPOINT="endpoint" (optional)
# USER_{username}_AWS_S3_REGION="region" (optional)